{
  "name": "@contentstack/apps-cli",
  "version": "0.0.0-alpha-1",
  "description": "App Cli",
  "author": "Contentstack CLI",
  "homepage": "https://github.com/contentstack/contentstack-apps-cli",
  "license": "MIT",
  "main": "./lib/index.js",
  "bin": {
    "app": "./bin/run"
  },
  "repository": "contentstack/apps-cli",
  "files": [
    "/bin",
    "/lib",
    "/npm-shrinkwrap.json",
    "/oclif.manifest.json"
  ],
  "dependencies": {
    "@contentstack/cli-command": "^1.2.9",
    "@contentstack/cli-utilities": "^1.4.5",
    "adm-zip": "^0.5.10",
    "chalk": "^4.1.2",
    "lodash": "^4.17.21",
    "open": "^9.1.0",
    "shelljs": "^0.8.5",
    "tmp": "^0.2.1",
    "winston": "^3.9.0"
  },
  "devDependencies": {
    "@oclif/test": "^2.0.3",
    "@types/adm-zip": "^0.5.0",
    "@types/chai": "^4.3.5",
    "@types/mocha": "^10.0.1",
    "@types/node": "^20.3.1",
    "@types/shelljs": "^0.8.12",
    "@types/tmp": "^0.2.3",
    "axios": "^1.4.0",
    "chai": "^4.3.7",
    "dotenv": "^16.3.1",
    "eslint": "^8.43.0",
    "eslint-config-oclif": "^4.0.0",
    "eslint-config-oclif-typescript": "^1.0.3",
    "mocha": "^10.2.0",
    "nyc": "^15.1.0",
    "oclif": "^3.9.1",
    "shx": "^0.3.4",
    "ts-node": "^10.9.1",
    "tslib": "^2.5.3",
    "typescript": "^5.1.3"
  },
  "oclif": {
    "bin": "csdx",
    "dirname": "apps-cli",
    "commands": "./lib/commands",
    "plugins": [],
    "topicSeparator": ":",
    "additionalHelpFlags": [
      "-h"
    ],
    "additionalVersionFlags": [
      "-v"
    ],
    "topics": {}
  },
  "scripts": {
    "build": "npm run clean && shx rm -rf lib && tsc -b",
    "lint": "eslint . --ext .ts --config .eslintrc",
    "postpack": "shx rm -f oclif.manifest.json",
    "posttest": "npm run lint",
    "prepack": "npm run build && oclif manifest && oclif readme",
    "test": "mocha --forbid-only \"test/**/*.test.ts\"",
    "version": "oclif readme && git add README.md",
    "clean": "rm -rf ./lib tsconfig.tsbuildinfo oclif.manifest.json",
    "test:unit:report": "nyc --extension .ts mocha --forbid-only \"test/unit/**/*.test.ts\""
  },
  "engines": {
    "node": ">=16"
  },
  "bugs": "https://github.com/contentstack/contentstack-apps-cli/issues",
  "keywords": [
    "oclif"
  ],
  "types": "./lib/index.d.ts",
  "csdxConfig": {
    "shortCommandName": {
      "app:get": "APGT",
      "app:create": "APCRT",
      "app:update": "APUPT"
    }
  }
}
