{
  "name": "@contentstack/apps-cli",
  "version": "0.0.0-alpha-1",
  "description": "App Cli",
  "author": "Contentstack CLI",
  "homepage": "https://github.com/contentstack/contentstack-apps-cli",
  "license": "MIT",
  "main": "./lib/index.js",
  "repository": "contentstack/apps-cli",
  "files": [
    "/bin",
    "/lib",
    "/npm-shrinkwrap.json",
    "/oclif.manifest.json"
  ],
  "dependencies": {
    "@contentstack/cli-command": "^1.2.1",
    "@contentstack/cli-utilities": "^1.3.0",
    "@oclif/plugin-help": "^5",
    "@oclif/plugin-plugins": "^2.3.2",
    "adm-zip": "^0.5.10",
    "chalk": "^4.1.2",
    "dotenv": "^16.0.3",
    "lodash": "^4.17.21",
    "open": "^8.4.2",
    "winston": "^3.8.2"
  },
  "devDependencies": {
    "@oclif/test": "^2.3.6",
    "@types/chai": "^4",
    "@types/mocha": "^9.0.0",
    "@types/node": "^16.18.12",
    "chai": "^4",
    "eslint": "^7.32.0",
    "eslint-config-oclif": "^4",
    "eslint-config-oclif-typescript": "^1.0.3",
    "mocha": "^9",
    "oclif": "^3",
    "shx": "^0.3.3",
    "ts-node": "^10.9.1",
    "tslib": "^2.5.0",
    "typescript": "^4.9.5"
  },
  "oclif": {
    "bin": "csdx",
    "dirname": "apps-cli",
    "commands": "./lib/commands",
    "plugins": [
      "@oclif/plugin-help",
      "@oclif/plugin-plugins"
    ],
    "topicSeparator": ":",
    "additionalHelpFlags": [
      "-h"
    ],
    "additionalVersionFlags": [
      "-v"
    ],
    "topics": {}
  },
  "scripts": {
    "build": "shx rm -rf lib && tsc -b",
    "lint": "eslint . --ext .ts --config .eslintrc",
    "postpack": "shx rm -f oclif.manifest.json",
    "posttest": "npm run lint",
    "prepack": "npm run build && oclif manifest && oclif readme",
    "test": "mocha --forbid-only \"test/**/*.test.ts\"",
    "version": "oclif readme && git add README.md"
  },
  "engines": {
    "node": ">=14.0.0"
  },
  "bugs": "https://github.com/contentstack/contentstack-apps-cli/issues",
  "keywords": [
    "oclif"
  ],
  "types": "./lib/index.d.ts",
  "csdxConfig": {
    "shortCommandName": {}
  }
}
